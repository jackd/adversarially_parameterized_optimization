{
  "dataset": {
    "type": "h3m",
    "filter_kwargs": {
        "camera_idxs": [1]
    },
    "normalize_kwargs": {
      "skeleton_id": "s14",
      "pixel_scale": 1000,
      "space_scale": 1000,
      "consistent_pose": true,
      "consistent_projection": true,
      "fps": 10
    }
  },
  "loss_type": "wasserstein",
  "n_z": 32,
  "activation": "relu",
  "n_g": [128, 64],
  "n_c": [128, 64],
  "batch_size": 128
}
