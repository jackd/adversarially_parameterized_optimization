{
  "dataset": {
    "type": "h3m",
    "filter_kwargs": {
        "camera_idxs": [1]
    },
    "normalize_kwargs": {
      "skeleton_id": "s24",
      "consistent_pose": true,
      "consistent_projection": true,
      "scale_to_height": true
    }
  },
  "loss_type": "wasserstein",
  "discriminator_clip_val": 0.01,
  "n_z": 32,
  "activation": "relu",
  "n_g": [128, 64],
  "n_c": [128, 64],
  "batch_size": 128
}
